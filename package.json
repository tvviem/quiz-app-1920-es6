{
  "name": "quiz-app-1920",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "./node_modules/.bin/mocha --require @babel/register ./tests/index",
    "server": "babel-node ./src/bin/www",
    "watch": "nodemon --config ./nodemon.json",
    "dev": "SET NODE_ENV=development && npm run watch ./src/bin/www",
    "clean": "rimraf dist",
    "build": "npm run clean && babel src --config-file ./.babelrc --out-dir dist --copy-files",
    "prod": "npm run build && SET NODE_ENV=production && node ./dist/bin/www"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "debug": "~4.1.1",
    "dotenv": "^8.2.0",
    "express": "~4.17.1",
    "http-errors": "~1.7.3",
    "mongoose": "^5.8.3",
    "morgan": "~1.9.1",
    "node-sass-middleware": "0.11.0",
    "pug": "^2.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/register": "^7.7.7",
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.0",
    "superagent": "^5.1.3"
  }
}
